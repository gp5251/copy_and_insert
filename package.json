{
  "name": "copy-and-insert",
  "displayName": "Copy & Insert",
  "publisher": "paulgui",
  "description": "copy a file to the target directory and insert the path to the file you're editting.将选中到文件拷贝到目标目录，并将文件路径插入到当前文档。",
  "version": "0.0.10",
  "keywords": [
    "copy",
    "insert",
    "paste"
  ],
  "engines": {
    "vscode": "^1.30.0"
  },
  "icon": "res/logo.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:copyAndInsert.func"
  ],
  "main": "./extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gp5251/copy_and_insert"
  },
  "contributes": {
    "commands": [
      {
        "command": "copyAndInsert.func",
        "title": "copy & insert"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "copyAndInsert",
      "properties": {
        "copyAndInsert.paths": {
          "type": "array",
          "default": ["./src/assets"],
          "description": "复制到目标文件夹,存在多个文件夹时会以选项形式供用户选择"
        },
        "copyAndInsert.aliasKey": {
          "type": "string",
          "default": "@",
          "description": "alias映射名,如果不需要请留空"
        },
        "copyAndInsert.alias": {
          "type": "string",
          "default": "./src",
          "description": "alias映射路径,如果不需要请留空"
        }
      },
      "keybindings": [
        {
          "command": "copyAndInsert.func",
          "key": "ctrl+f10",
          "mac": "ctrl+cmd+p",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "fs-extra": "^9.0.1"
  }
}

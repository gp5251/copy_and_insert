{
  "name": "copy-and-insert",
  "displayName": "copyAndInsert",
  "publisher": "paulgui",
  "description": "copy a file to the target directory you specified and insert the path to the file you're editting.方便在项目中拷贝文件，并插入文件路径到当前文档。",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:copy-and-insert.func"
  ],
  "main": "./extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gp5251/copy_and_insert"
  },
  "contributes": {
    "commands": [
      {
        "command": "copy-and-insert.func",
        "title": "copy & insert"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "copy-and-insert",
      "properties": {
        "copyAndInsert.path": {
          "type": "string",
          "default": "./src/assets",
          "description": "复制到文件夹"
        }
      },
      "keybindings": [
        {
          "command": "copy-and-insert.func",
          "key": "ctrl+f10",
          "mac": "ctrl+cmd+p",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "fs-extra": "^9.0.1"
  }
}
